doctype html
html
  head
    meta(charset='ISO-8859-1')
    title Property Guru
    link(rel='stylesheet', href='/styles/landing.css', type="text/css")
    link(href='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css', rel='stylesheet', integrity='sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm', crossorigin='anonymous')
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css')
    script(src='https://code.jquery.com/jquery-3.2.1.slim.min.js', integrity='sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN', crossorigin='anonymous')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js', integrity='sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q', crossorigin='anonymous')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js', integrity='sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl', crossorigin='anonymous')
    script(src='/scripts/landing.js')
    script(src='/scripts/propertyDetails.js')
  body
    div
      header
        nav.navbar.navbar-dark.navbar-expand-lg.bg-dark.fixed-top.scrolling-navbar
          #header_bar.container
            a.navbar-brand(href='/')
              img.logo(src='/images/logo.jpg')
            #basicExampleNav.collapse.navbar-collapse
              ul.navbar-nav.mr-auto
                li.nav-item
                  a.nav-link(href='/') Home
                li.nav-item
                  a.nav-link(href='/#services') Services
                li.nav-item
                  a.nav-link(href='/#contact') Contact
                li.nav-item
                  a.nav-link(href='/buySearch') Buy
                li.nav-item
                  a.nav-link(href='/property') Sell
                li.nav-item
                  a.nav-link(href='/dashboard') Dashboard
              ul.navbar-nav.ml-auto
                li.nav-item
                 a.nav-link(href='/userListings')
                    |  Hi #{name}
                li.nav-item
                  a.nav-link(href='/logout')
                    i.fa.fa-sign-out.nav-item(aria-hidden='true')
                    |  Logout
      // Main Navigation
      // Main layout
      main
        .container
          br
          section#services.text-center
          br
          br
          .row
            if(allList)
              each property in allList
                .col-lg-4.col-md-12.mb-4
                  .z-depth-1-half
                  a(href='#')
                    object(data=property.image, width='200', height='200')
                      img.img-fluid(src='/images/sell.jpg', alt='Nothing to display' )
                  h4.my-4.font-weight-bold.text-center #{property.propName}

                .col-lg-8.col-md-12.mb-4
                  .z-depth-1-half
                  a(href='#')
                  h4.my-4.font-weight-bold #{property.description} 
                  .tab
                    button.tablinks(onclick="openInfo(event, 'Information')") More Information

                  #Information.tabcontent.default
                    p
                    | Selling Price: $#{property.price}
                    hr
                    | House Address: #{property.addr1} #{property.addr2} #{property.city} #{property.state} #{property.zipcode}
                    hr
                    | House Type: #{property.proptype}
                    hr
                    | Furnishing : #{property.furntype}
                    hr
                    | Bedroom count: #{property.bedrooms}
                    hr
                    | County: #{property.county}
                    hr
                    | Total Area: #{property.area} Sq.Ft
                    hr
                    | Contact Owner: #{property.email} / #{property.phone}

      //Section: Contact end

      footer.page-footer.font-small.unique-color-dark
        .primary-color
          .container
            .row.py-4.d-flex.align-items-center
              .col-md-6.col-lg-5.text-center.text-md-left.mb-4.mb-md-0
                h6.mb-0.white-text
                  | Get connected with us on social
                  | networks!
              .col-md-6.col-lg-7.text-center.text-md-right
                a.fb-ic.ml-0
                  i.fa.fa-facebook.white-text.mr-4
                a.tw-ic
                  i.fa.fa-twitter.white-text.mr-4
                a.gplus-ic
                  i.fa.fa-google-plus.white-text.mr-4
                a.li-ic
                  i.fa.fa-linkedin.white-text.mr-4
                a.ins-ic
                  i.fa.fa-instagram.white-text.mr-lg-4
        .text-center.py-3
